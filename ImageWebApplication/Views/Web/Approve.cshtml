@model ImageWebApplication.Models.Approve
@{
    ViewBag.Title = "Approve";
}

<!DOCTYPE html>
<html>
<body>
    <h2 class="question">
        Are you sure you want to delete this handler?
    </h2>

    <center>
        <button type="button" id="OK" class="btn-danger" style="margin:50px; width: 150px; height: 100px; font-size:20px">OK</button>
        <button type="button" id="Cancel" class="btn-info" style="margin:50px; width: 150px; height: 100px; font-size:20px">Cancel</button>
    </center>
</body>
</html>

<script type="text/javascript">
        document.getElementById("OK").onclick = function () {
            document.getElementById("Cancel").disabled = true;
            document.getElementById("OK").disabled = true;
            location.href = "DeleteHandlerOK";
        };
</script>

<script type="text/javascript">
        document.getElementById("Cancel").onclick = function () {
            document.getElementById("Cancel").disabled = true;
            document.getElementById("OK").disabled = true;
            location.href = "DeleteHandlerCancel";
        };
</script>
